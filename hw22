A = input (' введите значение A ')
 введите значение A 
>>> B = input (' введите значение B ')
 введите значение B 
>>> C = input (' введите значение C ')
 введите значение C 
>>> if A < B and B > C :
	print (' выполняются оба неравенства ')
else :
	if A < B and B <= C :
		print (' выполняется только первое неравенство ')
	else :
		if A >= B and B > C :
			print (' выполняется только второе неравенство ')
		else :
			print (' оба неравенства не выполняются ')
